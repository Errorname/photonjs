datasource db {
  provider = "sqlite"
  url      = "file:db/migration_engine3.db"
  default  = true
}

generator photon {
  provider  = "photonjs"
  output    = "@generated/photon"
  transpile = false
}

model User {
  id       String @id @default(cuid())
  username String @unique
  posts    Post[]
}

model Post {
  id   String @id @default(cuid())
  data String
  user User?
}