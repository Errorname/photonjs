datasource db {
  provider = "sqlite"
  url      = "file:db/migration_engine.db"
  default  = true
}

generator photon {
  provider  = "photonjs"
  output    = "@generated/photon"
  transpile = false
}

model User {
  id       Int    @id
  username String @unique
  posts    Post[]
}

model Post {
  id   Int    @id
  data String
  user User
}